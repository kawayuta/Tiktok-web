<p id="notice"><%= notice %></p>

<h1>Tag Inflows</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Youtube</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tag_inflows.each do |tag_inflow| %>
      <tr>
        <td><%= tag_inflow.tag %></td>
        <td><%= tag_inflow.youtube %></td>
        <td><%= link_to 'Show', tag_inflow %></td>
        <td><%= link_to 'Edit', edit_tag_inflow_path(tag_inflow) %></td>
        <td><%= link_to 'Destroy', tag_inflow, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tag Inflow', new_tag_inflow_path %>
